{% extends 'base.html.twig' %}

{% block title %}Hello PerstationController!{% endblock %}

{% block body %}
    <h1>Créer une prestation</h1>

    {{ form_start(form) }}
        {{ (form_row(form.title)) }}
        {{ (form_row(form.price)) }}
        {{ (form_row(form.image)) }}
        {{ (form_row(form.description)) }}
        {{ (form_row(form.content)) }}
        <button type="submit" class="btn btn-primary">Créer</button>
    {{ form_end(form) }}
{% endblock %}
